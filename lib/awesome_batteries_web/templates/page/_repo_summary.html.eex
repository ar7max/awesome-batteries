
<a href="<%= AwesomeBatteriesWeb.PageView.repo_url(@repo) %>"> <%= @repo.name %> </a>
<span>
    <sub>        
        <%= @repo.stargazers_count %>
    </sub>
    <img width="4%" src="https://static-cdn.jtvnw.net/emoticons/v1/114836/3.0"/>
</span>
<span>
    <sub>
        <%= @repo.days %>
    </sub>
    <img width="4%" src="https://image.freepik.com/free-icon/calendar-with-word-day_318-9710.jpg"/>
</span>

<%= @repo.description |> Earmark.as_html! |> raw %>